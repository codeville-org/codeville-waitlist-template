{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@client/*": [
        "./src/client/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}